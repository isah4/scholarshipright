# Server Configuration
PORT=3001
NODE_ENV=development

# OpenAI Configuration
OPENAI_API_KEY=your_openai_api_key_here
OPENAI_MODEL=gpt-4

# SerpAPI Configuration (for web search)
SERPAPI_KEY=your_serpapi_key_here

# Rate Limiting
